services:
  itextosconfigdb:
    image: dk-reg.mas:5000/mariadb:1.0
    environment: [MYSQL_ALLOW_EMPTY_PASSWORD=yes, MYSQL_DATABASE=gw, TZ=Asia/Kolkata]
    restart: always
    volumes: ['/home/deployments/mysql/configdb/data:/var/lib/mysql', '/home/deployments/mysql/configdb/logs:/var/log/mysql/'
     ]
    ports: ['15001:3306']
version: '3'
